{
  "name": "jarvis-backend",
  "version": "1.0.0",
  "description": "Jarvis AI Assistant Backend - Voice-controlled IoT management system",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "test:unit": "jest --testPathPattern=test/unit",
    "test:integration": "jest --testPathPattern=test/integration",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "docker:build": "docker build -t jarvis-backend .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "logs": "docker-compose logs -f jarvis-backend",
    "setup": "node scripts/setup.js",
    "migrate": "node scripts/migrate.js"
  },
  "keywords": ["jarvis", "ai", "iot", "voice", "automation", "smart-home"],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "ws": "^8.14.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "speakeasy": "^2.0.0",
    "axios": "^1.6.2",
    "dockerode": "^4.0.0",
    "node-cron": "^3.0.3",
    "winston": "^3.11.0",
    "joi": "^17.11.0",
    "redis": "^4.6.10",
    "sqlite3": "^5.1.6",
    "multer": "^1.4.4",
    "dotenv": "^16.3.1",
    "yaml": "^2.3.4",
    "systeminformation": "^5.21.15",
    "node-telegram-bot-api": "^0.64.0",
    "form-data": "^4.0.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "eslint": "^8.56.0",
    "supertest": "^6.3.3",
    "@eslint/js": "^8.56.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/jarvis-hud.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/jarvis-hud/issues"
  },
  "homepage": "https://github.com/yourusername/jarvis-hud#readme"
